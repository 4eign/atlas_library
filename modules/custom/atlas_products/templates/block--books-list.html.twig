<div data-ng-books-list ng-init="uuid='{{ uuid }}'">
	<div ng-repeat="book in books['{{ uuid }}']">
    <div class="row">
      <div class="col s12 m7 l3">
        <div class="card ">
          <div class="card-image">
              <img src="{[{ book.image.uri }]}">
              <span class="card-title">{[{ book.name }]}</span>
              <a class="btn-floating halfway-fab waves-effect waves-light red"><i class="material-icons">+</i></a>
          </div>
          <div class="card-content">
            <p>{[{ book.description }]}</p>
          </div>
          <div class="card-action">
            <a href="#">This is a link</a>
          </div>
        </div>
      </div>
    </div>
	</div>
</div>